{
  "name": "signalk-units-preference",
  "version": "0.5.0-beta.3",
  "description": "SignalK webapp/utility for managing path value/unit conversions and display preferences",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint \"{src,public}/**/*.{ts,js}\"",
    "format": "prettier --write \"{src,public}/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"{src,public}/**/*.{ts,js,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "signalk-node-server-plugin",
    "signalk-category-utility",
    "signalk-webapp",
    "unit converter",
    "signalk-target units",
    "signalk-preferences"
  ],
  "author": "Maurice Tamman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/motamman/signalk-units-preference.git"
  },
  "bugs": {
    "url": "https://github.com/motamman/signalk-units-preference/issues"
  },
  "homepage": "https://github.com/motamman/signalk-units-preference#readme",
  "signalk": {
    "appSupport": "webapp",
    "compatibility": ">=2.13.0",
    "displayName": "Units Display Selector",
    "appIcon": "units.png"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^6.0.3",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@signalk/server-api": "^2.8.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "prettier": "^3.6.2",
    "typescript": "^5.3.0"
  },
  "signalk-plugin-enabled-by-default": true
}
